{
  "extends": null,
  "appId": "io.github.myhpwa.TwoFauth",
  "files": [
    "buildElectron/**/*",
    "package.json"
  ],
  "icon": "buildElectron/icon.icns",
  "mac": {
    "type": "distribution",
    "identity": null,
    "extendInfo": {
      "NSCameraUsageDescription": "此 app 須要您授權相機權限以掃描 QR code，且不會作任何資料收集的目的。"
    }
  },
  "mas": {
    "type": "distribution",
    "identity": "3rd Party Mac Developer",
    "artifactName": "${name}_${version}_mas64.${ext}",
    "category": "public.app-category.education",
    "minimumSystemVersion": "10.10",
    "hardenedRuntime": false,
    "entitlements": "buildElectron/entitlements.mas.plist",
    "entitlementsInherit": "buildElectron/entitlements.mas.inherit.plist",
    "provisioningProfile": "TwoFauth.provisionprofile"
  }
}