{
  "extends": null,
  "appId": "io.github.myhpwa.TwoFauth",
  "files": [
    "buildElectron/**/*",
    "package.json"
  ],
  "extraResources": [
    {
      "from": "electronBuilderConfigs",
      "to": ".",
      "filter": ["IsSnap.txt"]
    }
  ],
  "icon": "public/assets/icon/icon.png",
  "snap": {
    "artifactName": "${name}_${version}_linux64.${ext}",
    "category": "public.app-category.education",
    "plugs": [
      "default",
      "camera"
    ]
  }
}